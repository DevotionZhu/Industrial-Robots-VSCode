{
  "name": "Robot Programming Theme",
  "displayName": "Robot Programming",
  "description": "Robot Programming Highlighting for industrial robots",
  "icon": "icon.png",
  "version": "0.0.1",
  "publisher": "RoboDK Inc.",
  "engines": {
    "vscode": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboDK/Industrial-Robots-VSCode.git"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "contributes":
    {
    "grammars": [
        {
            "language": "ABB",
            "path": "./syntaxes/syntax.abb.json",
            "scopeName": "robodk.abb"
        },
        {
            "language": "Fanuc",
            "path": "./syntaxes/syntax.fanuc.json",
            "scopeName": "robodk.fanuc"
        },
        {
            "language": "Universal_Robots",
            "path": "./syntaxes/syntax.universal_robots.json",
            "scopeName": "robodk.universal_robots"
        },
        {
            "language": "KUKA",
            "path": "./syntaxes/syntax.kuka.json",
            "scopeName": "robodk.kuka"
        },
        {
            "language": "Motoman",
            "path": "./syntaxes/syntax.motoman.json",
            "scopeName": "robodk.motoman"
        },
        {
            "language": "Comau",
            "path": "./syntaxes/syntax.comau.json",
            "scopeName": "robodk.comau"
        },
        {
            "language": "Staubli",
            "path": "./syntaxes/syntax.staubli.json",
            "scopeName": "robodk.staubli"
        },
        {
            "language": "Kawasaki",
            "path": "./syntaxes/syntax.kawasaki.json",
            "scopeName": "robodk.kawasaki"
        }
    ],
    "languages": [
        {
            "aliases": [
                "ABB",
                "RAPID",
                "S4",
                "S4C",
                "IRC5"
            ],
            "configuration": "./cnfg.abb.json",
            "extensions": [
                ".sys",
                ".mod",
                ".prg",
                ".pgf"
            ],
            "id": "ABB"
        },
        {
            "aliases": [
                "Fanuc",
                "LS",
                "TP"
            ],
            "configuration": "./cnfg.fanuc.json",
            "extensions": [
                ".LS",
                ".ls"
            ],
            "id": "Fanuc"
        },
        {
            "aliases": [
                "KUKA",
                "KRC",
                "KRC2",
                "KRC4",
                "SRC",
                "DAT"
            ],
            "configuration": "./cnfg.kuka.json",
            "extensions": [
                ".src",
                ".dat"
            ],
            "id": "KUKA"
        },
        {
            "aliases": [
                "Motoman",
                "Yaskawa",
                "JBI",
                "Inform",
                "InformII",
                "InformIII"
            ],
            "configuration": "./cnfg.motoman.json",
            "extensions": [
                ".jbi",
                ".cnd"
            ],
            "id": "Motoman"
        },
        {
            "aliases": [
                "Universal",
                "Universal Robots",
                "universal robots",
				"Universal-Robots",
				"Universal_Robots",
				"universal_robots"
            ],
            "configuration": "./cnfg.universal_robots.json",
            "extensions": [
                ".script",
                ".urp"
            ],
            "id": "Universal_Robots"
        },
		{
            "aliases": [
                "Comau",
                "comau"
            ],
            "configuration": "./cnfg.comau.json",
            "extensions": [
                ".pdl"
            ],
            "id": "Comau"
        },
        {
            "aliases": [
                "Staubli",
                "Scara",
                "St√§ubli"
            ],
            "configuration": "./cnfg.staubli.json",
            "extensions": [
                ".pgx",
                ".pjx",
                ".dtx"
            ],
            "id": "Staubli"
        },
        {
            "aliases": [
                "Kawasaki"
            ],
            "configuration": "./cnfg.Kawasaki.json",
            "extensions": [
                ".pg"
            ],
            "id": "Kawasaki"
        }
    ],
    "snippets": [
        {
            "language": "ABB",
            "path": "./snippets/snippet.abb.json"
        },
        {
            "language": "Fanuc",
            "path": "./snippets/snippet.fanuc.json"
        },
        {
            "language": "KUKA",
            "path": "./snippets/snippet.kuka.json"
        },
        {
            "language": "Motoman",
            "path": "./snippets/snippet.motoman.json"
        },
        {
            "language": "Comau",
            "path": "./snippets/snippet.comau.json"
        },
        {
            "language": "Universal_Robots",
            "path": "./snippets/snippet.universal_robots.json"
        },
        {
            "language": "Staubli",
            "path": "./snippets/snippet.staubli.json"
        },
        {
            "language": "Kawasaki",
            "path": "./snippets/snippet.kawasaki.json"
        }	
    ]
}
}
