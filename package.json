{
  "name": "Robot Programming Theme",
  "displayName": "Robot Programming",
  "description": "Robot Programming Highlighting for industrial robots",
  "version": "0.0.1",
  "publisher": "RoboDK Inc.",
  "engines": {
    "vscode": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboDK/Industrial-Robots-VSCode.git"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "contributes":
    {
    "grammars": [
        {
            "language": "ABB RAPID",
            "path": "./syntaxes/robodk-abb-rapid.json",
            "scopeName": "robodk.abb-rapid"
        },
        {
            "language": "KUKA KRC",
            "path": "./syntaxes/robodk-kuka-krc.json",
            "scopeName": "robodk.kuka-krc"
        }
    ],
    "languages": [
        {
            "aliases": [
                "RAPID",
                "S4",
                "S4C",
                "IRC5"
            ],
            "configuration": "./abb-rapid-configuration.json",
            "extensions": [
                ".sys",
                ".mod",
                ".prg",
                ".pgf"
            ],
            "id": "ABB RAPID"
        },
        {
            "aliases": [
                "KUKA",
                "KRC2",
                "KRC4",
                "SRC",
                "DAT"
            ],
            "configuration": "./kuka-krc-configuration.json",
            "extensions": [
                ".src",
                ".dat"
            ],
            "id": "KUKA KRC"
        }
    ],
    "snippets": [
        {
            "language": "ABB RAPID",
            "path": "./snippets/robodk-abb-rapid.json"
        },
        {
            "language": "KUKA KRC",
            "path": "./snippets/robodk-kuka-krc.json"
        }
    ]
}
}
